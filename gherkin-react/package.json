{
  "name": "gherkin-react",
  "version": "1.0.0",
  "description": "React component for Gherkin documents",
  "main": "dist/src/gherkin-react.js",
  "repository": "git://github.com/cucumber/gherkin-react.git",
  "author": "Cucumber Limited <cukes@googlegroups.com>",
  "license": "MIT",
  "scripts": {
    "test": "mocha test/**/*.ts",
    "eslint-fix": "echo 'skipping eslint on generated code'",
    "coverage": "nyc --reporter=html --reporter=text mocha test/**/*.ts",
    "build": "babel src --out-dir dist/src",
    "build-test": "babel test --out-dir dist/test && tsc",
    "prepare": "npm run build",
    "postinstall": "node scripts/postinstall.js",
    "mocha-built": "mocha dist/test/**/*.js"
  },
  "dependencies": {
    "@types/react": "^16.4.12",
    "@types/react-dom": "^16.0.7",
    "cucumber-messages": "cucumber/cucumber-messages-javascript",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "styled-components": "^3.4.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-transform-async-to-generator": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@types/mocha": "^5.2.5",
    "awesome-typescript-loader": "^5.2.0",
    "eslint": "^5.3.0",
    "eslint-config-eslint": "^5.0.1",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-prettier": "^2.6.2",
    "mocha": "^5.2.0",
    "nyc": "^13.0.1",
    "prettier": "^1.8.2",
    "source-map-loader": "^0.2.4",
    "ts-node": "^7.0.1",
    "typescript": "^3.0.1",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0"
  }
}
