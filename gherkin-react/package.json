    "immutable": "^3.8.2",
    "protobufjs": "^6.8.8",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "simple-redux-websocket-middleware": "0.0.2",
    "styled-components": "^3.4.5",
    "typeface-roboto": "0.0.54"
