    "@types/styled-components": "^3.0.1",
    "classnames": "^2.2.6",
    "cucumber-messages": "^2.1.1",
    "enzyme": "^3.6.0",
    "immutable": "^4.0.0-rc.12",
    "protobufjs": "^6.8.8",
    "react": "^16.4.2",
